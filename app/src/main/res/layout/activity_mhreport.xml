<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MHReport">

    <ImageView
        android:id="@+id/vectorReport"
        android:layout_width="146dp"
        android:layout_height="309dp"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:layout_marginLeft="0dp"
        android:layout_marginTop="0dp"
        android:layout_marginRight="0dp"
        android:scaleType="fitXY"
        app:srcCompat="@drawable/vector_pago" />

    <ImageView
        android:id="@+id/vectorReport2"
        android:layout_width="105dp"
        android:layout_height="282dp"
        android:layout_alignParentRight="true"
        android:layout_alignParentBottom="true"
        android:layout_marginTop="0dp"
        android:layout_marginBottom="1dp"
        android:scaleType="fitXY"
        app:srcCompat="@drawable/fondopago2" />

    <ImageButton
        android:id="@+id/btnBackReport"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="20dp"
        android:layout_marginTop="20dp"
        android:layout_marginEnd="20dp"
        android:layout_marginBottom="20dp"
        android:background="#00FFFFFF"
        android:src="@drawable/back" />

    <AutoCompleteTextView
        android:id="@+id/fecha"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@id/txtFecha"
        android:layout_marginStart="80dp"
        android:layout_marginTop="-1dp"
        android:fontFamily="@font/poppins_light"
        android:text="20/04/2020" />

    <ImageView
        android:id="@+id/esteto"
        android:layout_width="91dp"
        android:layout_height="102dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentEnd="true"
        android:layout_alignParentBottom="true"
        android:layout_marginStart="20dp"
        android:layout_marginTop="18dp"
        android:layout_marginEnd="300dp"
        android:layout_marginBottom="19dp"
        app:srcCompat="@drawable/estetoscopio" />

    <ImageView
        android:id="@+id/linea"
        android:layout_width="136dp"
        android:layout_height="44dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentEnd="true"
        android:layout_alignParentBottom="true"
        android:layout_marginStart="134dp"
        android:layout_marginEnd="141dp"
        android:layout_marginBottom="15dp"
        app:srcCompat="@drawable/linea" />

    <TextView
        android:id="@+id/textmsg"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_marginStart="94dp"
        android:layout_marginTop="96dp"
        android:layout_marginEnd="114dp"
        android:fontFamily="@font/aclonica"

        android:text="¡Pago realizado con éxito!"
        android:textAlignment="center"
        android:textSize="20dp" />

    <AutoCompleteTextView
        android:id="@+id/paciente"
        android:layout_width="121dp"
        android:layout_height="wrap_content"
        android:layout_below="@id/txtPaciente"
        android:layout_alignParentStart="true"
        android:layout_alignParentEnd="true"
        android:layout_marginStart="129dp"
        android:layout_marginTop="1dp"
        android:layout_marginEnd="161dp"
        android:fontFamily="@font/poppins_light"
        android:text="Cody Fisher"
        android:textAlignment="center" />

    <TextView
        android:id="@+id/txtPaciente"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@id/vectorReport"
        android:layout_alignParentStart="true"
        android:layout_alignParentEnd="true"
        android:layout_marginStart="154dp"
        android:layout_marginTop="-136dp"
        android:layout_marginEnd="177dp"
        android:fontFamily="@font/coda"
        android:text="Paciente"
        android:textColor="#7C7676"
        android:textSize="20sp" />

    <TextView
        android:id="@+id/txtEmail"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@id/paciente"
        android:layout_alignParentStart="true"
        android:layout_alignParentEnd="true"
        android:layout_marginStart="118dp"
        android:layout_marginTop="7dp"
        android:layout_marginEnd="126dp"
        android:fontFamily="@font/coda"
        android:text="Correo Electronico"
        android:textColor="#7C7676"
        android:textSize="20sp" />

    <AutoCompleteTextView
        android:id="@+id/email"
        android:layout_width="266dp"
        android:layout_height="wrap_content"
        android:layout_below="@id/txtEmail"
        android:layout_alignParentStart="true"
        android:layout_alignParentEnd="true"
        android:layout_marginStart="58dp"
        android:layout_marginTop="5dp"
        android:layout_marginEnd="86dp"
        android:fontFamily="@font/poppins_light"
        android:text="cody.fisher@unmsm.edu.pe" />

    <TextView
        android:id="@+id/txtID"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@id/email"
        android:layout_alignParentStart="true"
        android:layout_alignParentEnd="true"
        android:layout_marginStart="189dp"
        android:layout_marginTop="-3dp"
        android:layout_marginEnd="203dp"
        android:fontFamily="@font/coda"
        android:text="ID"
        android:textColor="#7C7676"
        android:textSize="20sp" />

    <AutoCompleteTextView
        android:id="@+id/Idreport"
        android:layout_width="50dp"
        android:layout_height="wrap_content"
        android:layout_below="@id/txtID"
        android:layout_alignParentStart="true"
        android:layout_alignParentEnd="true"
        android:layout_marginStart="165dp"
        android:layout_marginTop="1dp"
        android:layout_marginEnd="196dp"
        android:fontFamily="@font/poppins_light"
        android:text="222" />

    <TextView
        android:id="@+id/txtFecha"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@id/Idreport"
        android:layout_marginStart="100dp"
        android:layout_marginTop="3dp"
        android:fontFamily="@font/coda"
        android:text="Fecha"
        android:textColor="#7C7676"
        android:textSize="20sp" />

    <TextView
        android:id="@+id/txtCode"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@id/Idreport"
        android:layout_marginStart="70dp"
        android:layout_marginLeft="65dp"
        android:layout_marginTop="3dp"
        android:layout_toRightOf="@id/fecha"
        android:fontFamily="@font/coda"
        android:text="Codigo"
        android:textColor="#7C7676"
        android:textSize="20sp" />

    <AutoCompleteTextView
        android:id="@+id/CodigoR"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@id/txtCode"
        android:layout_marginStart="60dp"
        android:layout_marginLeft="55dp"
        android:layout_marginTop="-1dp"
        android:layout_toRightOf="@id/fecha"
        android:fontFamily="@font/poppins_light"
        android:text="18200178" />

    <ImageView
        android:id="@+id/iconQR"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@id/fecha"
        android:layout_alignParentStart="true"
        android:layout_alignParentEnd="true"
        android:layout_marginStart="148dp"
        android:layout_marginTop="17dp"
        android:layout_marginEnd="161dp"
        app:srcCompat="@drawable/qr" />

</RelativeLayout>